{
    "manifest_version": 2,
    "name": "AZSOFT Extension",
    "description": "Customize default New Tab of browser",
    "author": "Mai Hoang Quoc Bao",
    "version": "1.0",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_icon": "img/icon.png"
    },
    "icons": {
        "16": "img/icon.png",
        "48": "img/icon.png",
        "128": "img/icon.png"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    }
}